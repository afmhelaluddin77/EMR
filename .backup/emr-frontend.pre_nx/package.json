{
  "name": "@emr-monorepo/emr-frontend",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "emr-frontend",
    "targets": {
      "build": {
        "executor": "@nx/vite:build",
        "outputs": ["{options.outputPath}"],
        "defaultConfiguration": "production",
        "options": {
          "outputPath": "dist/apps/emr-frontend"
        },
        "configurations": {
          "development": {
            "mode": "development"
          },
          "production": {
            "mode": "production"
          }
        }
      },
      "serve": {
        "executor": "@nx/vite:dev-server",
        "defaultConfiguration": "development",
        "options": {
          "buildTarget": "emr-frontend:build"
        },
        "configurations": {
          "development": {
            "buildTarget": "emr-frontend:build:development",
            "hmr": true
          },
          "production": {
            "buildTarget": "emr-frontend:build:production",
            "hmr": false
          }
        }
      },
      "preview": {
        "executor": "@nx/vite:preview-server",
        "defaultConfiguration": "development",
        "options": {
          "buildTarget": "emr-frontend:build"
        }
      },
      "test": {
        "executor": "@nx/vite:test",
        "outputs": ["{options.reportsDirectory}"],
        "options": {
          "passWithNoTests": true,
          "reportsDirectory": "../../coverage/apps/emr-frontend"
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint",
        "outputs": ["{options.outputFile}"]
      }
    }
  }
}
